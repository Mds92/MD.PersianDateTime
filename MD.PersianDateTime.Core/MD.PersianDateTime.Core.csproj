<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>C# library to use PersianCalendar as easy as DateTime</Description>
    <Copyright>By Mohammad Dayyan, 0903-3339923</Copyright>
    <AssemblyTitle>PersianDateTime</AssemblyTitle>
    <Authors>Mohammad Dayyan</Authors>
    <TargetFramework>netstandard1.6</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AssemblyName>MD.PersianDateTime.Core</AssemblyName>
    <PackageId>MD.PersianDateTime.Core</PackageId>
    <PackageLicenseUrl>http://www.gnu.org/licenses/gpl-3.0-standalone.html</PackageLicenseUrl>
    <NetStandardImplicitPackageVersion>1.6.1</NetStandardImplicitPackageVersion>
    <PackageTargetFallback>$(PackageTargetFallback);dnxcore50</PackageTargetFallback>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
  </PropertyGroup>

</Project>
